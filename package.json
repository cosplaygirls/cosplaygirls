{
  "name": "cosplaygirls",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "run-p start:**",
    "start:app": "react-scripts start",
    "start:lambda": "npm run yaml2json && netlify-lambda serve functions",
    "build": "run-p build:**",
    "build:app": "npm run clean:app && react-scripts build",
    "build:lambda": "npm run clean:lambda && npm run yaml2json && netlify-lambda build functions",
    "clean:app": "rimraf build",
    "clean:lambda": "rimraf lambda functions/cosplaygirls.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "yaml2json": "yaml2json cosplaygirls.yml > ./functions/cosplaygirls.json",
    "postbuild": "react-snap"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "netlify-lambda": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "react-snap": "^1.23.0",
    "rimraf": "^2.6.3",
    "yamljs": "^0.3.0"
  }
}
